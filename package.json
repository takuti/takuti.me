{
  "name": "takuti.me",
  "author": "takuti",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "build": "gulp",
    "watch": "gulp watch",
    "clean": "rm -rf public/",
    "prepare": "husky install"
  },
  "devDependencies": {
    "browser-sync": "^2.27.7",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-data": "^1.3.1",
    "gulp-kramdown": "^0.5.1",
    "gulp-plumber": "^1.2.1",
    "gulp-wrapper": "^1.0.0",
    "highlight.js": "^11.3.1",
    "husky": "^7.0.4",
    "imagemin-cli": "^7.0.0",
    "ini": "^2.0.0",
    "lint-staged": "^12.1.2",
    "lodash.template": ">=4.5.0",
    "minimist": "^1.2.5",
    "phantomjs-prebuilt": "^2.1.16",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "sync-request": "^6.1.0"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "lint-staged": {
    "*.{css,scss}": [
      "stylelint --fix"
    ],
    "*.{png,jpeg,jpg,gif}": [
      "node ./scripts/imagemin.js"
    ],
    "_content/{en,ja}/note/*.{md,html}": [
      "python ./scripts/recommend.py",
      "git add -u _content/en/note _content/ja/note"
    ],
    "static/cv/index.html": [
      "phantomjs ./scripts/toPDF.js 'static/cv/index.html' static/cv/Takuya_Kitazawa.pdf A4",
      "git add static/cv/Takuya_Kitazawa.pdf"
    ]
  }
}
