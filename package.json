{
  "name": "takuti.me",
  "author": "takuti",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "precommit": "lint-staged",
    "lint:css": "stylelint --fix \"_scss/**/*.{css,scss}\""
  },
  "dependencies": {},
  "devDependencies": {
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-data": "^1.2.1",
    "gulp-kramdown": "^0.5.1",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^4.0.1",
    "gulp-wrapper": "^1.0.0",
    "husky": "^0.14.3",
    "imagemin-cli": "^3.0.0",
    "lint-staged": "^7.0.4",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "sync-request": "^6.0.0"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "lint-staged": {
    "_scss/**/*.{css,scss}": [
      "stylelint --fix",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif}": [
      "node ./scripts/imagemin.js",
      "git add"
    ],
    "_content/note/*.{md,html}": [
      "python ./scripts/recommend.py",
      "git add _content/note"
    ]
  }
}
