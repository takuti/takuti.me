{
  "name": "takuti.me",
  "author": "takuti",
  "license": "MIT",
  "version": "1.0.0",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "lint:css": "stylelint --fix \"_scss/**/*.{css,scss}\""
    }
  },
  "dependencies": {},
  "devDependencies": {
    "del": "^3.0.0",
    "gulp": "^4.0.1",
    "gulp-data": "^1.2.1",
    "gulp-kramdown": "^0.5.1",
    "gulp-plumber": "^1.2.1",
    "gulp-sass": "^4.0.2",
    "gulp-wrapper": "^1.0.0",
    "highlight.js": "^9.13.1",
    "husky": "^1.3.1",
    "imagemin-cli": "^4.0.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.12.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "sync-request": "^6.0.0"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "lint-staged": {
    "_scss/**/*.{css,scss}": [
      "stylelint --fix",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif}": [
      "node ./scripts/imagemin.js",
      "git add"
    ],
    "_content/note/*.{md,html}": [
      "python ./scripts/recommend.py",
      "git add _content/note"
    ]
  }
}
