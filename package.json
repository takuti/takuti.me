{
  "name": "takuti.me",
  "author": "takuti",
  "license": "MIT",
  "version": "1.0.0",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "lint:css": "stylelint --fix \"_scss/**/*.{css,scss}\""
    }
  },
  "dependencies": {},
  "devDependencies": {
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-data": "^1.3.1",
    "gulp-kramdown": "^0.5.1",
    "gulp-plumber": "^1.2.1",
    "gulp-sass": "^4.1.0",
    "gulp-wrapper": "^1.0.0",
    "highlight.js": "^10.2.0",
    "husky": "^4.3.0",
    "imagemin-cli": "^6.0.0",
    "lint-staged": "^10.3.0",
    "node-sass": "^4.14.1",
    "stylelint": "^13.7.0",
    "stylelint-config-standard": "^20.0.0",
    "sync-request": "^6.1.0"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "lint-staged": {
    "_scss/**/*.{css,scss}": [
      "stylelint --fix",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif}": [
      "node ./scripts/imagemin.js",
      "git add"
    ],
    "_content/{en,ja}/note/*.{md,html}": [
      "python ./scripts/recommend.py",
      "git add _content/en/note _content/ja/note"
    ]
  }
}
