{
  "name": "takuti.me",
  "author": "takuti",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "build": "gulp",
    "watch": "gulp watch",
    "prepare": "husky install"
  },
  "devDependencies": {
    "browser-sync": "^2.27.5",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-data": "^1.3.1",
    "gulp-kramdown": "^0.5.1",
    "gulp-plumber": "^1.2.1",
    "gulp-sass": "^5.0.0",
    "gulp-wrapper": "^1.0.0",
    "highlight.js": "^11.2.0",
    "husky": "^7.0.0",
    "imagemin-cli": "^7.0.0",
    "ini": "^2.0.0",
    "lint-staged": "^11.1.2",
    "minimist": "^1.2.5",
    "node-sass": "^6.0.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "sync-request": "^6.1.0"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "lint-staged": {
    "_scss/**/*.{css,scss}": [
      "stylelint --fix",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif}": [
      "node ./scripts/imagemin.js",
      "git add"
    ],
    "_content/{en,ja}/note/*.{md,html}": [
      "python ./scripts/recommend.py",
      "git add -u _content/en/note _content/ja/note"
    ]
  }
}
